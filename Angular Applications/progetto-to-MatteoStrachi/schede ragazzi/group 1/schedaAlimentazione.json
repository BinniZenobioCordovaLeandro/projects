  "code": "schedaAlimentazione",
  "html": "<section>\n  <div class=\"panel panel-primary\">\n    <div class=\"panel-heading\">\n      <label>Informazione Generale</label>\n    </div>\n    <div class=\"panel-body\">\n\n      <div id=\"DIABETICO\" class=\"row\">\n        <div class=\"col-xs-3 col-sm-2 col-lg-1\">\n          <label>DIABETICO</label>\n        </div>\n        <div class=\"col-xs-9 col-sm-10 col-lg-11\">\n          <label class=\"ui-radio\" ng-click=\"formData.alimentazione.diabetico={code:'opa0',value:'SI',score:null}\"><input name=\"DIABETICO\" type=\"radio\" value=\"opa0\" ng-model=\"formData.alimentazione.diabetico.code\"><span>SI</span></label>\n          <label class=\"ui-radio\" ng-click=\"formData.alimentazione.diabetico={code:'opb0',value:'NO',score:null}\"><input name=\"DIABETICO\" type=\"radio\" value=\"opb0\" ng-model=\"formData.alimentazione.diabetico.code\"><span>NO</span></label>\n          <input type=\"text\" summary=\"Alim_Diabetico\" ng-model=\"formData.alimentazione.diabetico.value\" ng-show=\"false\">\n        </div>\n      </div>\n\n      <div id=\"DISFAGICO\" class=\"row\">\n        <div class=\"col-xs-3 col-sm-2 col-lg-1\"><label>DISFAGICO</label></div>\n        <div class=\"col-xs-9 col-sm-10 col-lg-11\">\n          <div class=\"row\">\n            <div class=\"col-xs-12\">\n              <label class=\"ui-radio\" ng-click=\"formData.alimentazione.disfagico.attivo={code:'op0a',value:'SI',score:null}\"><input type=\"radio\" value=\"op0a\" ng-model=\"formData.alimentazione.disfagico.attivo.code\"><span>SI</span></label>\n              <label class=\"ui-radio\" ng-click=\"formData.alimentazione.disfagico.attivo={code:'op0b',value:'NO',score:null}\"><input type=\"radio\" value=\"op0b\" ng-model=\"formData.alimentazione.disfagico.attivo.code\"><span>NO</span></label>\n              <input type=\"text\" summary=\"Alim_Disfagico\" ng-model=\"formData.alimentazione.disfagico.attivo.code\" ng-show=\"false\">\n            </div>\n          </div>\n          <div class=\"row\" ng-hide=\"formData.alimentazione.disfagico.attivo != 'op0a'\">\n            <div class=\"col-xs-12\">\n              <label class=\"ui-checkbox\" ng-click=\"formData.alimentazione.disfagico.tipo={code:'disf_solidi',score:null}\"><input type=\"checkbox\" ng-model=\"formData.alimentazione.disfagico.liquidi\"><span>DISFAGICO AI LIQUIDI</span></label>\n              <label class=\"ui-checkbox\" ng-click=\"formData.alimentazione.disfagico.tipo={code:'disf_solidi',score:null}\"><input type=\"checkbox\" value=\"disf_solidi\" ng-model=\"formData.alimentazione.disfagico.solidi\"><span>DISFAGICO AI SOLIDI</span></label>\n              <input type=\"text\" summary=\"Alim_Disfagico_Tipo\" ng-model=\"formData.alimentazione.disfagico.tipo.code\" ng-show=\"false\">\n            </div>\n          </div>\n        </div>\n      </div><!-- END DISFAGICO -->\n\n      <div id=\"INTEGRATORE\" class=\"row\">\n        <div class=\"col-xs-3 col-sm-2 col-lg-1\">\n          <label>INTEGRATORE</label>\n        </div>\n        <div class=\"col-xs-9 col-sm-4 col-lg-3\">\n          <div class=\"ui-select\">\n            <select ng-model=\"formData.alimentazione.integratore\" ng-options=\"item.value for item in viewData.listIntegratore track by item.code\">\n            </select>\n            <input type=\"text\" summary=\"Alim_Integratore\" ng-model=\"formData.alimentazione.integratore.value\" ng-show=\"false\">\n          </div>\n        </div>\n      </div>\n\n      <div id=\"chivi\" class=\"row\">\n\n        <div id=\"COLAZIONE\" class=\"col-xs-12 col-sm-6 col-lg-4\">\n          <div class=\"panel panel-primary\">\n            <div class=\"panel-heading\">\n              <label>COLAZIONE</label>\n            </div>\n            <div class=\"panel-body\">\n\n              <div id=\"BEVE\" class=\"row\">\n                <div class=\"col-xs-5\">\n                  <label>BEVE</label>\n                </div>\n                <div class=\"col-xs-7\">\n                  <div class=\"ui-select\">\n                    <select ng-model=\"formData.alimentazione.colazione.beve\" ng-options=\"item.value for item in viewData.listbebida track by item.code\">\n                    </select>\n                    <input type=\"text\" summary=\"Alim_ColazioneBeve\" ng-model=\"formData.alimentazione.colazione.beve.value\" ng-show=\"false\">\n                  </div>\n                </div>\n              </div> <!-- end BEVE -->\n              <div id=\"MANGIA\" class=\"row\">\n                <div class=\"col-xs-5\">\n                  <label>MANGIA</label>\n                </div>\n                <div class=\"col-xs-7\">\n                  <div class=\"ui-select\">\n                    <select ng-model=\"formData.alimentazione.colazione.mangia\" ng-options=\"item.value for item in viewData.listMangia track by item.code\">\n                    </select>\n                    <input type=\"text\" summary=\"Alim_ColazioneMangia\" ng-model=\"formData.alimentazione.colazione.mangia.value\" ng-show=\"false\">\n                  </div>\n                </div>\n              </div> <!-- end MANGIA -->\n            </div>\n          </div>\n        </div> <!-- end COLAZIONE -->\n\n\n        <div id=\"PRANZO\" class=\"col-xs-12 col-sm-6 col-lg-4\">\n          <div class=\"panel panel-primary\">\n            <div class=\"panel-heading\">\n              <label>PRANZO</label>\n            </div>\n            <div class=\"panel-body\">\n\n              <div id=\"PANE_PRANZO\" class=\"row\">\n                <div class=\"col-xs-5\">\n                  <label>PANE</label>\n                </div>\n                <div class=\"col-xs-7\">\n                  <div class=\"ui-select\">\n                    <select ng-model=\"formData.alimentazione.pranzo.pane\" ng-options=\"item.value for item in viewData.listPane track by item.code\">\n                    </select>\n                    <input type=\"text\" summary=\"Alim_PranzoPane\" ng-model=\"formData.alimentazione.pranzo.pane.value\" ng-show=\"false\">\n                  </div>\n                </div>\n              </div> <!-- end PANE_PRANZO -->\n\n              <div id=\"VERDURA_CRUDA_PRANZO\" class=\"row\">\n                <div class=\"col-xs-5\"><label>VERDURA CRUDA</label></div>\n                <div class=\"col-xs-7\">\n                  <label class=\"ui-radio\" ng-click=\"formData.alimentazione.pranzo.verduraCruda={code:'op1a',value:'SI',score:null}\"><input type=\"radio\" value=\"op1a\" ng-model=\"formData.alimentazione.pranzo.verduraCruda.code\"><span>SI</span></label>\n                  <label class=\"ui-radio\" ng-click=\"formData.alimentazione.pranzo.verduraCruda={code:'op1b',value:'NO',score:null}\"><input type=\"radio\" value=\"op1b\" ng-model=\"formData.alimentazione.pranzo.verduraCruda.code\"><span>NO</span></label>\n                  <input type=\"text\" summary=\"Alim_PranzoVerdura\" ng-model=\"formData.alimentazione.pranzo.verduraCruda.value\" ng-show=\"false\">\n                </div>\n              </div><!--END VERDURA_CRUDA_PRANZO-->\n\n              <div id=\"VINO_PRANZO\" class=\"row\">\n                <div class=\"col-xs-5\"><label>VINO</label></div>\n                <div class=\"col-xs-7\">\n                  <label class=\"ui-radio\" ng-click=\"formData.alimentazione.pranzo.vino={code:'op2a',value:'SI',score:null}\"><input type=\"radio\" value=\"op2a\" ng-model=\"formData.alimentazione.pranzo.vino.code\"><span>SI</span></label>\n                  <label class=\"ui-radio\" ng-click=\"formData.alimentazione.pranzo.vino={code:'op2b',value:'NO',score:null}\"><input type=\"radio\" value=\"op2b\" ng-model=\"formData.alimentazione.pranzo.vino.code\"><span>NO</span></label>\n                  <input type=\"text\" summary=\"Alim_PranzoVino\" ng-model=\"formData.alimentazione.pranzo.vino.value\" ng-show=\"false\">\n                </div>\n              </div><!--END VINO_PRANZO-->\n\n              <div id=\"CAFFE_PRANZO\" class=\"row\">\n                <div class=\"col-xs-5\"><label>CAFFE</label></div>\n                <div class=\"col-xs-7\">\n                  <label class=\"ui-radio\" ng-click=\"formData.alimentazione.pranzo.caffe={code:'op3a',value:'SI',score:null}\"><input type=\"radio\" value=\"op3a\" ng-model=\"formData.alimentazione.pranzo.caffe.code\"><span>SI</span></label>\n                  <label class=\"ui-radio\" ng-click=\"formData.alimentazione.pranzo.caffe={code:'op3b',value:'NO',score:null}\"><input type=\"radio\" value=\"op3b\" ng-model=\"formData.alimentazione.pranzo.caffe.code\"><span>NO</span></label>\n                  <input type=\"text\" summary=\"Alim_PranzoCaffe\" ng-model=\"formData.alimentazione.pranzo.caffe.value\" ng-show=\"false\">\n                </div>\n              </div><!--END CAFFE_PRANZO-->\n\n              <div id=\"FRUTTA_PRANZO\" class=\"row\">\n                <div class=\"col-xs-5\">\n                  <label>FRUTTA</label>\n                </div>\n                <div class=\"col-xs-7\">\n                  <div class=\"ui-select\">\n                    <select ng-model=\"formData.alimentazione.pranzo.frutta\" ng-options=\"item.value for item in viewData.listPranzoFrutta track by item.code\">\n                    </select>\n                    <input type=\"text\" summary=\"Alim_PranzoFrutta\" ng-model=\"formData.alimentazione.pranzo.frutta.value\" ng-show=\"false\">\n                  </div>\n                </div>\n              </div> <!-- end FRUTTA_PRANZO -->\n            </div>\n          </div>\n        </div> <!-- end PRANZO -->\n\n\n        <div id=\"MERENDA\" class=\"col-xs-12 col-sm-6 col-lg-4\">\n          <div class=\"panel panel-primary\">\n            <div class=\"panel-heading\">\n              <label>MERENDA</label>\n            </div>\n            <div class=\"panel-body\">\n\n              <div id=\"PRENDE\" class=\"row\">\n                <div class=\"col-xs-5\">\n                  <label>PRENDE</label>\n                </div>\n                <div class=\"col-xs-7\">\n                  <div class=\"ui-select\">\n                    <select ng-model=\"formData.alimentazione.merenda.prende\" ng-options=\"item.value for item in viewData.listMerenda track by item.code\">\n                    </select>\n                    <input type=\"text\" summary=\"Alim_MerendaPrende\" ng-model=\"formData.alimentazione.merenda.prende.value\" ng-show=\"false\">\n                  </div>\n                </div>\n              </div> <!-- end PRENDE-->\n            </div>\n          </div>\n        </div> <!-- end MERENDA -->\n\n\n\n\n        <div id=\"CENA\" class=\"col-xs-12 col-sm-6 col-lg-4\">\n          <div class=\"panel panel-primary\">\n            <div class=\"panel-heading\">\n              <label>CENA</label>\n            </div>\n            <div class=\"panel-body\">\n\n              <div id=\"PANE_CENA\" class=\"row\">\n                <div class=\"col-xs-5\">\n                  <label>PANE</label>\n                </div>\n                <div class=\"col-xs-7\">\n                  <div class=\"ui-select\">\n                    <select ng-model=\"formData.alimentazione.cena.pane\" ng-options=\"item.value for item in viewData.listPane track by item.code\">\n                    </select>\n                    <input type=\"text\" summary=\"Alim_CenaPane\" ng-model=\"formData.alimentazione.cena.pane.value\" ng-show=\"false\">\n                  </div>\n                </div>\n              </div> <!-- end PANE_CENA -->\n\n              <div id=\"VERDURA_CRUDA_CENA\" class=\"row\">\n                <div class=\"col-xs-5\"><label>VERDURA CRUDA</label></div>\n                <div class=\"col-xs-7\">\n                  <label class=\"ui-radio\" ng-click=\"formData.alimentazione.cena.verduraCruda={code:'op4a',value:'SI',score:null}\"><input type=\"radio\" value=\"op4a\" ng-model=\"formData.alimentazione.cena.verduraCruda.code\"><span>SI</span></label>\n                  <label class=\"ui-radio\" ng-click=\"formData.alimentazione.cena.verduraCruda={code:'op4b',value:'NO',score:null}\"><input type=\"radio\" value=\"op4b\" ng-model=\"formData.alimentazione.cena.verduraCruda.code\"><span>NO</span></label>\n                  <input type=\"text\" summary=\"Alim_CenaVerdura\" ng-model=\"formData.alimentazione.cena.verduraCruda.value\" ng-show=\"false\">\n                </div>\n              </div><!--END VERDURA_CRUDA_CENA-->\n\n              <div id=\"VINO_CENA\" class=\"row\">\n                <div class=\"col-xs-5\"><label>VINO</label></div>\n                <div class=\"col-xs-7\">\n                  <label class=\"ui-radio\" ng-click=\"formData.alimentazione.cena.vino={code:'op5a',value:'SI',score:null}\"><input type=\"radio\" value=\"op5a\" ng-model=\"formData.alimentazione.cena.vino.code\"><span>SI</span></label>\n                  <label class=\"ui-radio\" ng-click=\"formData.alimentazione.cena.vino={code:'op5b',value:'NO',score:null}\"><input type=\"radio\" value=\"op5b\" ng-model=\"formData.alimentazione.cena.vino.code\"><span>NO</span></label>\n                  <input type=\"text\" summary=\"Alim_CenaVino\" ng-model=\"formData.alimentazione.cena.vino.value\" ng-show=\"false\">\n                </div>\n              </div><!--END VINO_CENA-->\n\n              <div id=\"INFUSO_CENA\" class=\"row\">\n                <div class=\"col-xs-5\"><label>INFUSO</label></div>\n                <div class=\"col-xs-7\">\n                  <label class=\"ui-radio\" ng-click=\"formData.alimentazione.cena.infuso={code:'op6a',value:'SI',score:null}\"><input type=\"radio\" value=\"op6a\" ng-model=\"formData.alimentazione.cena.infuso.code\"><span>SI</span></label>\n                  <label class=\"ui-radio\" ng-click=\"formData.alimentazione.cena.infuso={code:'op6b',value:'NO',score:null}\"><input type=\"radio\" value=\"op6b\" ng-model=\"formData.alimentazione.cena.infuso.code\"><span>NO</span></label>\n                  <input type=\"text\" summary=\"Alim_CenaInfuso\" ng-model=\"formData.alimentazione.cena.infuso.value\" ng-show=\"false\">\n                </div>\n              </div><!--END INFUSO_CENA-->\n\n              <div id=\"FRUTTA_CENA\" class=\"row\">\n                <div class=\"col-xs-5\">\n                  <label>FRUTTA</label>\n                </div>\n                <div class=\"col-xs-7\">\n                  <div class=\"ui-select\">\n                    <select ng-model=\"formData.alimentazione.cena.frutta\" ng-options=\"item.value for item in viewData.listCenaFrutta track by item.code\">\n                    </select>\n                    <input type=\"text\" summary=\"Alim_CenaFrutta\" ng-model=\"formData.alimentazione.cena.frutta.value\" ng-show=\"false\">\n                  </div>\n                </div>\n              </div> <!-- end FRUTTA_CENA -->\n            </div>\n          </div>\n        </div> <!-- end CENA -->\n\n      </div> <!-- end Chivi -->\n\n    </div><!-- END PANEL BODY -->\n  </div><!-- END PANEL -->\n</section>",
